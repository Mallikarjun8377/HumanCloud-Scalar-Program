package DataStructure;


public class CustomStack {
	
	int capacity=1;
	int stack[]=new int[capacity];
	int size;
	
	public void push(int data) {
		if(size==capacity);
		Expand();
	}


	private void Expand() {
		int lengh=size;
		int newStack[]=new int[capacity*2];
	    for(int i=0;i<stack.length;i++) {
	       newStack[i]=stack[i];
	    }
		
	}

}
