package DataStructure;

public class NewLinkedList {
	
	Node head;
	public class Node{
		int data;
		Node next;
		
		Node(int data){
			this.data=data;
			next=null;
		}
		
	}
	 public void add(int data) {
		 Node newNode=new Node(data);
		 newNode.data=data;
		 newNode.next=null;
		 
		 if(head==null) {
			 head=newNode;			 
		 }else {
			 Node current=head;
			 
			 while(current.next!=null) {
				 current=current.next;
			 }
			 current=newNode;
		 } 
	}
  
	 public  void display() {
		 Node current=head;
		 if(current.next!=null) {
			 System.out.println(" " +current.data);
			 current=current.next;
		 }
	 }
	 
	 
}
