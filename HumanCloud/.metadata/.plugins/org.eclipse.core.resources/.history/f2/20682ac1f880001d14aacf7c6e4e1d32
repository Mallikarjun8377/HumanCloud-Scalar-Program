package DataStructure;

public class CustomStack {
	
	private int maxSize;
	private int[] stack;
	private int top;
		
	public CustomStack(int size) {
		maxSize=size;
		stack=new int[maxSize];
		top=-1;
	}
		
	public void push(int data) {
		if(top==maxSize-1) {
			top++;
			stack[top]=data;			
		}else {
			resize(maxSize*2);
			push(data);
			
		}
	}
	private void resize(int newSize) {
		int[] newArray=new int[newSize];
		for(int i=0;i<stack.length;i++){
			newArray[i]=stack[i];
			stack=newArray;			
		}
		maxSize=newSize;
		}
	
	private int pop() {
		if(top!=maxSize-1) {
	}

}
